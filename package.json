{
  "name": "@koreanpanda/discord.js-framework",
  "version": "1.0.2",
  "description": "A framework for Discord.js",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "npx tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "Discord.js",
    "Bot Framework",
    "Discord Bot"
  ],
  "author": "Cody Spratford",
  "license": "MIT",
  "devDependencies": {
    "@koreanpanda/inscriber": "^2.0.4",
    "@types/jest": "^26.0.20",
    "@types/ws": "^7.4.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "ascii-table": "0.0.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koreanpanda345/discord.js-framework.git"
  },
  "bugs": {
    "url": "https://github.com/koreanpanda345/discord.js-framework/issues"
  },
  "homepage": "https://github.com/koreanpanda345/discord.js-framework#readme"
}
